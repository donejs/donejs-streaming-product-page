{
  "name": "donejs-streaming-product-page",
  "version": "0.0.0",
  "description": "An awesome DoneJS app",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donejs-user/donejs-streaming-product-page.git"
  },
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "private": true,
  "scripts": {
    "api": "node api/server.js",
    "test": "testee test.html --browsers firefox --reporter Spec",
    "start": "done-serve --port 8080 --proxy http://localhost:8084 --strategy incremental --key ~/.localhost-ssl/private.pem --cert ~/.localhost-ssl/cert.pem",
    "develop": "done-serve --develop --port 8080 --proxy http://localhost:8084",
    "develop-static": "done-serve --static --port 8080 --proxy http://localhost:8084",
    "debug": "DONE_SSR_DEBUG=1 node --debug-brk --inspect node_modules/.bin/done-serve --port 8080 --proxy http://localhost:8084 --strategy incremental --key ~/.localhost-ssl/private.pem --cert ~/.localhost-ssl/cert.pem",
    "debug-client": "DONE_SSR_DEBUG=1 done-serve --port 8080 --proxy http://localhost:8084 --strategy incremental --key ~/.localhost-ssl/private.pem --cert ~/.localhost-ssl/cert.pem",
    "build": "node build"
  },
  "main": "index.js",
  "files": [
    "src"
  ],
  "keywords": [
    "donejs-app"
  ],
  "steal": {
    "directories": {
      "lib": "src"
    },
    "configDependencies": [
      "node_modules/can-zone/register"
    ],
    "plugins": [
      "done-css",
      "done-component",
      "steal-less",
      "steal-stache"
    ],
    "envs": {
      "server-production": {
        "renderingBaseURL": "/dist"
      }
    },
    "serviceBaseURL": ""
  },
  "dependencies": {
    "can-component": "^3.0.7",
    "can-connect": "^1.3.8",
    "can-connect-ndjson": "0.0.0",
    "can-define": "^1.0.17",
    "can-ndjson-stream": "^0.1.6",
    "can-route": "^3.0.8",
    "can-route-pushstate": "^3.0.3",
    "can-set": "^1.1.0",
    "can-stache": "^3.0.20",
    "can-view-autorender": "^3.0.4",
    "can-zone": "^0.6.6",
    "detect-node": "^2.0.3",
    "done-autorender": "^1.3.0-beta.1",
    "done-component": "^1.0.0",
    "done-css": "^3.0.0",
    "done-serve": "^1.4.0-zbeta.0",
    "generator-donejs": "^1.0.0",
    "steal": "^1.3.1",
    "steal-less": "^1.2.0",
    "steal-stache": "^3.0.5"
  },
  "devDependencies": {
    "donejs-cli": "^1.0.0",
    "funcunit": "^3.2.0",
    "steal-qunit": "^1.0.1",
    "steal-tools": "^1.2.0",
    "testee": "^0.3.0",
    "can-fixture": "^1.0.13"
  },
  "license": "MIT"
}
